{
  "name": "sc-b2c-web-vue3-vite",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "test": "cypress open-ct",
    "test:ci": "cypress run-ct",
    "test:e2e": "start-server-and-test dev http-get://localhost:3000 cypress:open",
    "test:e2e:ci": "start-server-and-test dev http-get://localhost:3000 cypress",
    "generate": "graphql-codegen --config graphql-codegen.yml",
    "lint:style": "stylelint src/**/*.{css,scss,vue}",
    "lint:script": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vincent687/storybook": "^0.0.0",
    "axios": "^0.24.0",
    "graphql": "^16.0.1",
    "villus": "^1.0.1",
    "vue": "^3.2.16",
    "vue-i18n": "^9.2.0-beta.2",
    "vue-router": "4",
    "vue-router-layout": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@cypress/vite-dev-server": "^2.2.1",
    "@cypress/vue": "^3.0.5",
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/typescript": "^2.4.1",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@testing-library/cypress": "^8.0.2",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "autoprefixer": "9",
    "babel-loader": "^8.2.3",
    "cypress": "^9.1.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.1.1",
    "flush-promises": "^1.0.2",
    "graphql-tag": "^2.12.6",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.2",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.4",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.0",
    "puppeteer": "^11.0.0",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.21.0",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.5.2",
    "vite": "^2.6.4",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint --fix",
    "*.{css,scss,vue}": "stylelint --fix",
    "*": "prettier -w -u"
  },
  "license": "MIT"
}
